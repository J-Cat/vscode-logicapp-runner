{
  "name": "vscode-logicapp-runner",
  "displayName": "VSCode LogicApp Runner Extension",
  "description": "",
  "version": "1.0.0",
  "author": {
    "name": "J-Cat",
    "url": "https://github.com/J-Cat"
  },
  "publisher": "j-cat",
  "license": "CC-BY-NC-4.0",
  "repository": {
    "url": "https://github.com/J-Cat/vscode-logicapp-runner"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:logicapp-runner.runTrigger"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Logic App Runner",
      "properties": {
        "logicapp-runner.baseUrl": {
          "type": "string",
          "description": "Base URL for your Azure Functions (default of http://localhost:7071",
          "default": "http://localhost:7071"
        }
      }
    },
    "commands": [
      {
        "command": "logicapp-runner.runTrigger",
        "title": "Logic App: Run Trigger"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@azure/arm-logic": "^8.1.0",
    "axios": "^1.1.3",
    "http-status-codes": "^2.2.0"
  }
}
